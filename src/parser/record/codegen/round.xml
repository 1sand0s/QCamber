<symbol name="RoundSymbol" id="r">
  <pattern><![CDATA[
  r([0-9.]+)
  ]]></pattern>

  <private_block><![CDATA[
  qreal m_r;
  ]]></private_block>

  <constructor><![CDATA[
  m_r = caps[1].toDouble() / 1000.0 / 2.0;
  ]]></constructor>

  <painterPath><![CDATA[
  m_cachedPath.addEllipse(QPointF(0, 0), m_r, m_r);
  ]]></painterPath>
</symbol>
