<symbol name="SquareSymbol" id="s">
  <pattern><![CDATA[
  s([0-9.]+)
  ]]></pattern>

  <private_block><![CDATA[
  qreal m_s;
  ]]></private_block>

  <constructor><![CDATA[
  m_s = caps[1].toDouble() / 1000.0;
  ]]></constructor>

  <painterPath><![CDATA[
  path.addRect(-m_s / 2, -m_s / 2, m_s, m_s);
  ]]></painterPath>
</symbol>
