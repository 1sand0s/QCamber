<symbol name="EllipseSymbol" id="el">
  <pattern><![CDATA[
  el([0-9.]+)x([0-9.]+)
  ]]></pattern>

  <private_block><![CDATA[
  qreal m_w;
  qreal m_h;
  ]]></private_block>

  <constructor><![CDATA[
  m_w = caps[1].toDouble() / 1000.0;
  m_h = caps[2].toDouble() / 1000.0;
  ]]></constructor>

  <painterPath><![CDATA[
  m_cachedPath.addEllipse(-m_w / 2, -m_h / 2, m_w, m_h);
  ]]></painterPath>
</symbol>
