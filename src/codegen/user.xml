<symbol name="UserSymbol" id="r">
  <pattern><![CDATA[
  r([0-9.]+)
  ]]></pattern>

  <private_block><![CDATA[
  qreal m_d;
  ]]></private_block>

  <constructor><![CDATA[
  m_d = caps[1].toDouble();
  ]]></constructor>

  <boundingRect><![CDATA[
  return QRectF(-m_d / 2, -m_d / 2, m_d, m_d);
  ]]></boundingRect>

  <paint><![CDATA[
  painter->setPen(QPen(Qt::red, 0));
  painter->setBrush(Qt::red);
  painter->drawEllipse(-m_d / 2, -m_d / 2, m_d, m_d);
  ]]></paint>
</symbol>
